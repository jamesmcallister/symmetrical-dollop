{
  "name": "symmetrical-dollop",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:jamesmcallister/symmetrical-dollop.git",
  "author": "James McAllister <jrfmcallister@gmail.com>",
  "license": "unlicensed",
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "presets": [
      "es2015",
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "modules": true
      }
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": false
        }
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "es2015"
      ],
      "react"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-prettier": "2.0.1",
    "eslint-plugin-react": "^7.0.0",
    "prettier": "^1.3.1",
    "prettier-eslint": "^6.2.0",
    "prettier-eslint-cli": "^3.4.3"
  },
  "scripts": {
    "format": "prettier-eslint --write \"app/**/*.js\"",
    "purge": "rm -rf ./node_modules && yarn",
    "start": "babel-node ./src/index.js"
  }
}